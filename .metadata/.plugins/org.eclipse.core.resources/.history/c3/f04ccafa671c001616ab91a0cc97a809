package com.infy.demo.experian;

import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.bind.annotation.RestController;



@SpringBootApplication
@RestController
public class ApiMainApplication {
	
	 public static void main(String[] args)
	 {
		 SpringApplication.run(ApiMainApplication.class, args);
	 }
	 @RequestMapping(value="/validate-address")
	 public String returnHello()
	 {
		 return "Hello this is first spring demo in dockler by Anand";
	 }

}
